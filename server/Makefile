##################################################################### 
#   Variables				  										#
#####################################################################

OBJECTS = *.o
BINARY = sic_server

##################################################################### 
#   Options 							#
#####################################################################

CC = gcc
CFLACS = -o

##################################################################### 
#   Rules							#
#####################################################################

all: sic_server 

sic_server:
	$(CC) $(CFLACS) $(BINARY) sic_server.c 
clean:
	rm -f $(BINARY) $(OBJECTS)
